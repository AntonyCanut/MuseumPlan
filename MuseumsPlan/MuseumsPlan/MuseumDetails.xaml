<?xml version="1.0" encoding="UTF-8"?>
<ContentPage 	BackgroundColor="White"
				xmlns ="http://xamarin.com/schemas/2014/forms"
				xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
				x:Class="MuseumsPlan.MuseumDetails">
	<ContentPage.Content>
		<StackLayout Padding="20,0,20,20">
			<Image 	Source="./Ressources/Drawable/Title.png"/>
			<Label 	Text="{Binding nom_du_musee}"/>
			<Label 	Text="{Binding ville}"
					TextColor="Aqua"/>
			<Label 	Text="{Binding ville}"/>
			<Label 	Text="{Binding periode_ouverture}"/>
			<Label 	Text="{Binding ferme}"/>
			<Label 	Text="{Binding sitweb}"/>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>
